# Firebase Configuration
# Get these values from your Firebase project console
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Stripe Configuration
# Get these values from your Stripe dashboard
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# YouTube API
# Get this from Google Cloud Console
REACT_APP_YOUTUBE_API_KEY=your_youtube_api_key_here

# MailerSend Configuration
# Get these values from your MailerSend dashboard
REACT_APP_MAILERSEND_API_KEY=your_mailersend_api_key_here
REACT_APP_MAILERSEND_DOMAIN=your_domain.com
REACT_APP_MAILERSEND_FROM_EMAIL=noreply@your_domain.com
REACT_APP_MAILERSEND_FROM_NAME="Reverse Aging Academy"

# Google Analytics 4
# Get this from your Google Analytics 4 property settings
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Optional: App Configuration
REACT_APP_APP_NAME="The Reverse Aging Challenge"
REACT_APP_APP_URL=https://your-domain.com 